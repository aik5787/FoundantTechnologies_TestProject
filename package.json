{
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^21.0.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.3",
    "@faker-js/faker": "^9.1.0",
    "concurrently": "^9.0.1",
    "cypress": "^13.15.1",
    "dotenv": "^16.4.5",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
"scripts": {
  "clean:reports": "rm -rf cypress/reports/*",
  "test:addTask": "cypress run --browser chrome --spec 'cypress/e2e/UI/addTask/**/*'",
  "test:addNote": "cypress run --browser chrome --spec 'cypress/e2e/UI/addNote/**/*'",
  "test:login": "cypress run --browser chrome --spec 'cypress/e2e/UI/login/**/*'",
  "test:logout": "cypress run --browser chrome --spec 'cypress/e2e/UI/logout/**/*'",
  "test:createProfile": "cypress run --browser chrome --spec 'cypress/e2e/UI/createProfile/**/*'",
  "merge:reports": "mochawesome-merge ./cypress/reports/*.json > ./cypress/reports/merged-report.json || true",
  "generate:html": "marge cypress/reports/merged-report.json -o cypress/reports/html --inline",
  "test:parallel": "concurrently \"npm run test:addTask\" \"npm run test:addNote\" \"npm run test:login\" \"npm run test:logout\" \"npm run test:createProfile\"",
  "test:full": "npm run clean:reports && npm run test:parallel && npm run merge:reports && npm run generate:html"
}





}
